{
  "name": "formkit",
  "version": "0.5.8",
  "description": "Smart framework agnostic form helper.",
  "repository": "git@github.com:bozonx/formkit.git",
  "license": "MIT",
  "engines": {
    "node": ">=9.x"
  },
  "author": "Ivan Kozyrin <ipkozyrin@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha",
    "build": "yarn test && yarn run build-web && yarn run build-web-min",
    "build-web": "NODE_ENV=production node_modules/.bin/webpack-cli --progress --config ./webpack.web.config.js",
    "build-web-min": "NODE_ENV=production node_modules/.bin/webpack-cli -p --progress --config ./webpack.web-min.config.js",
    "cov": "./node_modules/.bin/istanbul cover --report none ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test",
    "cov-html": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test && sensible-browser ./coverage/lcov-report/index.html",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test",
    "doc-html": "esdoc -c esdoc.json && sensible-browser ./esdoc/index.html",
    "doc": "esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint ./src",
    "fix": "node_modules/.bin/eslint --fix ./src"
  },
  "dependencies": {
    "eventemitter3": "^3.0.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coffee-coverage": "^3.0.0",
    "coffeescript": "2.2.2",
    "eslint": "^4.18.2",
    "eslint-config-effency": "^0.1.8",
    "eslint-config-google": "^0.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mocha-sinon": "^2.0.0",
    "mock-require": "^3.0.1",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  }
}
