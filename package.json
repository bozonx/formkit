{
  "name": "formkit",
  "version": "1.0.4",
  "description": "Smart framework agnostic form helper.",
  "repository": "git@github.com:bozonx/formkit.git",
  "license": "MIT",
  "engines": {
    "node": ">=9.x"
  },
  "author": "Ivan Kozyrin <ipkozyrin@gmail.com>",
  "main": "dist/formkit.module.js",
  "scripts": {
    "test": "mocha",
    "build": "yarn test && yarn run build-module && yarn run build-web && yarn run build-web-min",
    "build-module": "webpack-cli --mode development --progress --config ./webpack.module.config.js",
    "build-web": "webpack-cli     --mode development --progress --config ./webpack.web.config.js",
    "build-web-min": "webpack-cli --mode production  --progress --config ./webpack.web-min.config.js",
    "cov": "istanbul cover --report none ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test",
    "cov-html": "istanbul cover ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test && sensible-browser ./coverage/lcov-report/index.html",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --require coffee-coverage/register-istanbul test",
    "doc-html": "esdoc -c esdoc.json && sensible-browser ./esdoc/index.html",
    "doc": "jsdoc -c ./jsdoc.conf.js",
    "lint": "eslint ./src",
    "fix": "eslint --fix ./src"
  },
  "dependencies": {
    "eventemitter3": "^3.0.1",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coffee-coverage": "^3.0.0",
    "coffeescript": "2.2.2",
    "docdash": "^0.4.0",
    "eslint": "^4.18.2",
    "eslint-config-effency": "^0.1.8",
    "eslint-config-google": "^0.9.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.1",
    "mocha-sinon": "^2.0.0",
    "mock-require": "^3.0.1",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  }
}
